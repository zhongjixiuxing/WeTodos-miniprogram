.i-collapse-item{
    width: 100%;

}
.i-collapse-item-title{
    vertical-align:middle;
    padding-left: 10px;
    font-weight: bold;
    width: 80%;
}

.wrap-wrap{
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    width: 100%;

}
.left {
    width: 70%;
}

.icons{
    display: flex;
    justify-content: center;
    align-items: center;
}

.i-collapse-item-title-wrap{
    position: relative;

    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 10px 10px;
    font-size: 28rpx;
}
.i-collapse-item-content{
    display:none;
}
.i-collapse-item-show-content{
    display:block;
}
.i-collapse-item-arrow{transition:transform .2s ease-in-out}
.i-collapse-item-arrow-show{transition:transform .2s ease-in-out;transform:rotate(90deg)}
